# 📁 파일: .env.example
# 환경 변수 예시 파일 - 이 파일을 복사하여 .env로 만들고 값을 채우세요

# Discord 봇 설정
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_bot_client_id_here
GUILD_ID=your_test_guild_id_here # 개발용 길드 ID (선택사항)

# 웹 대시보드 URL
WEB_URL=https://aimdot.dev
# 개발 환경에서는 http://localhost:5173 사용

# API 서버 설정
API_PORT=3000
JWT_SECRET=your_jwt_secret_key_here_change_this_in_production

# API 키 (선택사항) - 형식: name:key,name2:key2
API_KEYS=dashboard:your_api_key_here

# 데이터베이스 설정 (Prisma)
DATABASE_URL="postgresql://username:password@localhost:5432/aimdot_bot?schema=public"
# 또는 SQLite 사용 시
# DATABASE_URL="file:./dev.db"

# 로깅 설정
LOG_LEVEL=info # debug, info, warn, error
DISCORD_LOG_WEBHOOK=https://discord.com/api/webhooks/xxx/yyy # 선택사항
DISCORD_LOG_LEVEL=error # Discord로 전송할 최소 로그 레벨

# 환경 설정
NODE_ENV=development # development, production

# CDN 설정
IMAGE_CDN_URL=https://i.imgur.com

# 기본 이미지 URL
DEFAULT_THUMBNAIL=https://i.imgur.com/Sd8qK9c.gif

# Redis 설정 (선택사항 - 캐싱용)
REDIS_URL=redis://localhost:6379

# Sentry 설정 (선택사항 - 에러 추적)
SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# 개발 설정
DEBUG=false